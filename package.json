{
    "name": "@srsergio/taptapp-ar",
    "version": "1.0.2",
    "description": "AR Visualizer and Compiler for Astro and React",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/srsergiolazaro/taptapp-ar.git"
    },
    "bugs": {
        "url": "https://github.com/srsergiolazaro/taptapp-ar/issues"
    },
    "homepage": "https://github.com/srsergiolazaro/taptapp-ar#readme",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./astro/*": "./src/astro/*.astro",
        "./compiler/*": "./src/compiler/*.js"
    },
    "files": [
        "dist",
        "src",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build"
    },
    "peerDependencies": {
        "aframe": ">=1.5.0",
        "astro": ">=4.0.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "three": ">=0.160.0"
    },
    "dependencies": {
        "@tensorflow/tfjs": "^4.22.0",
        "@tensorflow/tfjs-backend-cpu": "^4.22.0",
        "@tensorflow/tfjs-backend-webgl": "^4.22.0",
        "lucide-react": "^0.477.0",
        "ml-matrix": "^6.10.4",
        "nanoid": "^5.0.7",
        "svd-js": "^1.1.1",
        "tinyqueue": "^2.0.3"
    },
    "devDependencies": {
        "@tensorflow/tfjs-node": "^4.22.0",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/three": "^0.170.0",
        "astro": "5.16.4",
        "jimp": "^1.6.0",
        "typescript": "^5.4.5",
        "vitest": "^4.0.16"
    },
    "publishConfig": {
        "access": "public"
    }
}
